{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "public/build/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "vercel-prerender.js",
      "use": "@vercel/node"
    },
    {
      "src": "vercel.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/prerender",
      "dest": "/vercel-prerender.js"
    },
    {
      "src": "/",
      "dest": "/vercel.js"
    },
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
