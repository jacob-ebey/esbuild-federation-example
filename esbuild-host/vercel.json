{
  "version": 2,
  "builds": [
    {
      "src": "public/build/**",
      "use": "@vercel/static"
    },
    {
      "src": "vercel.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/vercel.js"
    },
    {
      "source": "/build/(.*)",
      "destination": "/public/build/$1"
    }
  ]
}
